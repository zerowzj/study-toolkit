<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="urn:org:jgroups"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:org:jgroups http://www.jgroups.org/schema/JGroups-4.0.xsd">
    <!-- ==================== -->
    <!--     传输协议          -->
    <!-- ==================== -->
    <TCP bind_addr="custom:com.company.project.jgroups.BindAddr"
         bind_port="7800"/>
    <!-- ==================== -->
    <!--     初始成员发现协议   -->
    <!-- ==================== -->
    <TCPPING initial_hosts="10.60.0.65[7800],10.60.0.67[7800]"
             port_range="2"/>
    <!-- ==================== -->
    <!--     网络分区合并协议   -->
    <!-- ==================== -->
    <MERGE3 max_interval="10000"
            min_interval="5000"
            check_interval="15000"/>
    <!-- ==================== -->
    <!--     故障检测协议       -->
    <!-- ==================== -->
    <!--  -->
    <FD/>
    <!--  -->
    <FD_SOCK/>
    <!--  -->
    <FD_ALL/>
    <!--  -->
    <VERIFY_SUSPECT timeout="1500"/>
    <!-- ==================== -->
    <!--     可靠消息传输协议   -->
    <!-- ==================== -->
    <!-- 组播消息可靠性保证（重发）和FIFO消息序列保证 -->
    <pbcast.NAKACK2 use_mcast_xmit="false"
                    discard_delivered_msgs="true"/>
    <!-- 单播消息可靠性保证（重发）和FIFO消息序列保证 -->
    <UNICAST3/>
    <!-- ==================== -->
    <!--     消息稳定协议      -->
    <!-- ==================== -->
    <pbcast.STABLE/>
    <!-- ==================== -->
    <!--     组成员协议        -->
    <!-- ==================== -->
    <pbcast.GMS print_local_addr="true"
                print_physical_addrs="true"
                join_timeout="3000"/>
    <!-- ==================== -->
    <!--     流量控制协议       -->
    <!-- ==================== -->
    <UFC/>
    <!-- ==================== -->
    <!--     非阻塞流量控制协议 -->
    <!-- ==================== -->
    <UFC_NB/>
    <!-- ==================== -->
    <!--     分裂协议          -->
    <!-- ==================== -->
    <FRAG3/>
    <!-- ==================== -->
    <!--     排序协议       -->
    <!-- ==================== -->
    <SEQUENCER2/>
    <!-- ==================== -->
    <!--     状态转换协议       -->
    <!-- ==================== -->
    <pbcast.STATE_TRANSFER/>
    <!-- ==================== -->
    <!--            -->
    <!-- ==================== -->
    <pbcast.FLUSH />
    <!-- ==================== -->
    <!--     安全协议          -->
    <!-- ==================== -->
</config>